<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tko.co.jp.amoadtest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.amoad.AMoAdInterstitialVideoActivity"/>
        <activity android:name=".BannerActivity" android:label="バナー" />
        <activity android:name=".InterstitialActivity" android:label="インタースティシャル" />
        <activity android:name=".InfeedAfiOActivity" android:label="インフィードAfiO" />
        <activity android:name=".InterstitialAfiOActivity" android:label="インタースティシャルAfiO" />
        <activity android:name=".WebViewUrlActivity" android:label="WebView(URL指定)" />
        <activity android:name=".WebViewSidActivity" android:label="WebView(SID指定)" />
    </application>

</manifest>